#include <kipr/botball.h>
//DECLARING VARIABLES//
int l_motor=2;
int r_motor=3;
int speed=45;



//FUNCTION DEFINITONS//

void forward();
{  motor(l_motor,speed);
    motor(r_motor,speed);
}

void backward();
{	motor(l_motor,-speed);
    motor(r_motor,-speed);
}
    
int main()
    int fruitloops;
    forward();
    backward();

for(fruitloops=1;<=12;+=2)

{
    printf("Loopy\n");
    return 0;
}
